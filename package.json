{
  "name": "Cartoon-Hangover-Clon",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "parcel src/**/index.html",
    "save-dev": "npm install --save-dev @babel/core @babel/cli @babel/preset-env @babel/plugin-transform-runtime sass postcss-modules autoprefixer cssnano",
    "save": "npm install --save @babel/runtime core-js include-media hamburgers",
    "build": "parcel build ./src/index.html --out-dir docs",
    "deploy": "firebase deploy"
  },
  "keywords": [
    "javascript",
    "firebase"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "ie > 9"
        ]
      }
    }
  },
  "author": "Victor Alvarez <vicventum.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "core-js": "^3.6.5",
    "hamburgers": "^1.1.3",
    "include-media": "^1.4.9",
    "materialize-css": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "~9.8.6",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^3.2.2",
    "sass": "^1.26.11"
  },
  "@fullhuman/postcss-purgecss": {
    "content": [
      "./dist/*.html",
      "./dist/*.js",
      "https://kit-free.fontawesome.com/releases/latest/css/free.min.css"
    ],
    "css": [
      "./dist/*.css"
    ]
  }
}
